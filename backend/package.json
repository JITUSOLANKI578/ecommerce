{
  "name": "ambika-ecommerce-backend",
  "version": "1.0.0",
  "description": "Ambika E-Commerce Backend with Node.js, Express, and MongoDB",
  "main": "server.js",
  "scripts": {
  "dev": "nodemon server.js",
  "start": "node server.js",
  "seed": "node src/seeders/seedData.js",
  "seed:dev": "NODE_ENV=development node src/seeders/seedData.js",
  "seed:prod": "NODE_ENV=production node src/seeders/seedData.js",
  "seed:products": "node src/seeder.js",
  "seed:destroy": "node src/seeder.js -d",
  "seed:reset": "npm run seed:destroy && npm run seed"
}

  "dependencies": {
    "@elastic/elasticsearch": "^9.1.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.16",
    "razorpay": "^2.9.6",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.9.1",
    "nodemon": "^3.1.9",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0"
  },
  "keywords": [
    "ecommerce",
    "nodejs",
    "express",
    "mongodb",
    "backend"
  ],
  "author": "Ambika Team",
  "license": "MIT"
}
